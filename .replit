entrypoint = "index.js"
modules = ["nodejs-20", "php", "python-3.11", "php-8.2"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]
deploymentTarget = "gce"
ignorePorts = false
run = ["sh", "-c", "npm start"]

[[ports]]
localPort = 3000
externalPort = 8080

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "TFB Layout Test"

[[workflows.workflow]]
name = "TFB Layout Test"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php -S 0.0.0.0:5000 cfa-fire-forecast-plugin/test-tfb-layouts.php"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
