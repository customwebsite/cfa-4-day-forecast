entrypoint = "index.js"
modules = ["nodejs-20", "php", "python-3.11"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]
deploymentTarget = "gce"
ignorePorts = false
run = ["sh", "-c", "npm start"]

[[ports]]
localPort = 3000
externalPort = 8080

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 45021
externalPort = 3000

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Plugin Documentation"

[[workflows.workflow]]
name = "Plugin Documentation"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m http.server 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
